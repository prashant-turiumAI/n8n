{
  "name": "@n8n/temporal-worker",
  "version": "2.2.0",
  "description": "Temporal worker for n8n workflow execution",
  "main": "dist/start.js",
  "module": "src/start.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "start": "node dist/start.js",
    "dev": "pnpm build && pnpm start",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p ./tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "biome format --write src",
    "format:check": "biome ci src",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint --fix .",
    "watch": "tsc-watch -p tsconfig.build.json --onCompilationComplete \"tsc-alias -p tsconfig.build.json\""
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./start": {
      "require": "./dist/start.js",
      "import": "./src/start.ts",
      "types": "./dist/start.d.ts"
    },
    ".": {
      "require": "./dist/index.js",
      "import": "./src/index.ts",
      "types": "./dist/index.d.ts"
    },
    "./workflows": {
      "require": "./dist/workflows/index.js",
      "import": "./src/workflows/index.ts",
      "types": "./dist/workflows/index.d.ts"
    },
    "./src/workflows": {
      "require": "./dist/workflows/index.js",
      "import": "./src/workflows/index.ts",
      "types": "./dist/workflows/index.d.ts"
    }
  },
  "dependencies": {
    "@n8n/config": "workspace:*",
    "@n8n/di": "workspace:*",
    "@n8n/errors": "workspace:*",
    "@temporalio/worker": "^1.10.0",
    "@temporalio/client": "^1.10.0",
    "@temporalio/workflow": "^1.10.0",
    "@temporalio/activity": "^1.10.0",
    "n8n-workflow": "workspace:*",
    "n8n-core": "workspace:*"
  },
  "devDependencies": {
    "@n8n/typescript-config": "workspace:*",
    "@types/node": "^20.0.0"
  }
}


