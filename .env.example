# Temporal Configuration
# Set to true to enable Temporal workflow execution
TEMPORAL_ENABLED=true

# Secret key for authenticating Temporal worker with n8n credential API
# This must match between n8n backend and Temporal worker
# Generate a secure secret with: openssl rand -hex 32
# IMPORTANT: Replace this with your own generated secret!
TEMPORAL_WORKER_CREDENTIAL_SECRET=your-secret-here

# Optional: Temporal server configuration (defaults shown)
# TEMPORAL_HOST=localhost:7233
# TEMPORAL_NAMESPACE=default
# TEMPORAL_TASK_QUEUE=n8n-workflows
# TEMPORAL_TLS_ENABLED=false
